@model User

<body>
  <h1 class="login">Register</h1>
<form class="form-group register" asp-action="Register" asp-controller="Home" method="post">
        <span asp-validation-for="FirstName"></span><br>
        <label asp-for="FirstName"></label><br>
        <input class="form-control" asp-for="FirstName"><br>
        <span asp-validation-for="LastName"></span><br>
        <label asp-for="LastName"></label><br>
        <input class="form-control" asp-for="LastName"><br>
        <span asp-validation-for="Email"></span><br>
        <label asp-for="Email"></label><br>
        <input class="form-control" asp-for="Email"><br>
        <span asp-validation-for="Password"></span><br>
        <label asp-for="Password"></label><br>
        <input class="form-control" asp-for="Password"><br>
        <span asp-validation-for="Confirm"></span><br>
        <label asp-for="Confirm"></label><br>
        <input class="form-control" asp-for="Confirm"><br>
        <br>
      <button class="btn btn-primary">Register</button>
</form>

<partial class="login-partial" name="LoginUserPartial" />

</body>